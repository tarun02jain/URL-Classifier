{
  "manifest_version": 3,
  "name": "URL Classifier",
  "version": "1.0",
  "description": "Classify URLs based on extracted features.",
  "permissions": ["activeTab", "http://127.0.0.1:5000/*", "http://localhost:5173/*",
    "https://*/*" ],
   "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
  
    }
  },
  "icons": {

  },
  "host_permissions": [
    "http://localhost:5173/*"
  ]
}
